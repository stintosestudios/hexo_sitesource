<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title><%= config.title %></title>

    <!-- global css -->
    <%- css('css/global.css') %>

</head>
<body>

    <div class="container_main">

        <div class="container_header">

            <!-- menu links -->
            <p class="menu_links">
            <% for (var i in theme.menu){ %>

                <a href="<%- url_for(theme.menu[i]) %>" ><%= i %></a>

            <% } %>
            </p>

        </div>

        <div class="container_content">

            <%- body %>

        </div>

    </div>

    <%- partial('_partial/google-analytics') %>

</body>
</html>

